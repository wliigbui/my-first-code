//4、让用户输入其年龄，然后显示该年龄包含多少个月
void test04()
{
	int age;
	cin >> age;
	cout << age * 12 <<"个月"<< endl;
}

//5、

//6、编写一个函数，以光年为参数，并返回对应天文单位的值
long test06(double &g)
{
	
	cout << g << "light years = " << g * 63240 << "astronomical units" << endl;
}

//7、用户输入小时数和分钟数。传递给一个void（）函数
void test07(int h,int m)
{
	cout << "Enter the number of hours: " << h << endl;
	cout << "Enter the number of minutes: " << m << endl;
	cout << "Time: " << h << " : " << m << endl;
}
