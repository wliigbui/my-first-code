#include<stdio.h>                   //  浮点数的二进制储存方式
int main()
{
    float f = 5.5;
    //二进制：101.1;
    //S=0;   判断是否正负  0为正，1为负；
    //M=1.011;  第一个1 不储存到二进制;
    //E=2+127;    正负都加127；
    //结果：0100 0000 1011 0000 0000 0000 0000 0000
    //十六进制表示为:0x40b00000
    //内存：0x000000F07ACFFAF4  00 00 b0 40  ..?@
    return 0;
}
